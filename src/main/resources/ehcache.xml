    <cache alias="com.wondertek.cpm.domain.UserCost" uses-template="simple"/>

    <cache alias="com.wondertek.cpm.domain.HolidayInfo" uses-template="simple"/>

    <cache alias="com.wondertek.cpm.domain.ProjectInfo" uses-template="simple"/>

